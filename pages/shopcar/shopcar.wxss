
@import "./../produce/produce.wxss";





.bgcart{
  width: 40%;
  height: 270rpx;
  display: block;
  margin: 40% auto
}

.foot{
  width: 730rpx;
  height: 90rpx;
  position: fixed;
  display: flex;
  justify-content:space-between;
  bottom: 0;
  border-top: 2rpx #eeeeee solid;
  line-height: 90rpx;
  font-size: 32rpx;
  background-color: white;
  padding-left:20rpx 
}
.money{
  color: red;
  margin-left: 20rpx;
}
.close{
  width: 30%;
  background-color: #ff8247; 
  color: white;
  text-align: center;
}
.commodity{
  display: flex;
width: 95%;
  height: 190rpx;
  justify-content: space-between;
  margin: 20rpx 100rpx 20rpx 20rpx;
  border-bottom: 2rpx#eeeeee solid;
  position: relative;
}
.commodity image{
  width: 170rpx;
  height: 170rpx;
}
.titlecart{
  display: block;
  width: 250rpx;
  overflow: hidden;

}
.titles{
   display: block;
   height: 30%;
   width: 220rpx;
    font-size: 24rpx;
   -webkit-box-orient: vertical;
   overflow: hidden;
   word-break:break-all;
   display: -webkit-box;
 text-overflow: ellipsis;
 -webkit-line-clamp:2;
 margin-bottom:5%; 
 margin-top:5%;
}
.sizing{
  font-size: 20rpx;
  width: 220rpx;
   height: 28%;
   display: -webkit-box;
   -webkit-box-orient: vertical;
   overflow: hidden;
   word-break:break-all;
 text-overflow: ellipsis;
 -webkit-line-clamp:2;
}
.pice{
  font-size: 28rpx;
  height: 15%;
}

.calculate{
  height: 34rpx;
}
.shopNumber{
  margin-top: 100rpx
}
.ssss{
  line-height: 170rpx
}
.group{
  position: absolute;
  right: 10rpx;
  top: 5rpx;
  font-size: 24rpx;
  color: #c6c6c6
}
.item_container {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
}
 
.select_icon {
  width: 40rpx;
  height: 40rpx;
}

.ssss  image{
  width: 40rpx;
  height: 40rpx;
  border-radius:50% 
}
.we{margin-bottom: 90rpx}