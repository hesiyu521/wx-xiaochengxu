     <view class='address' wx:for='{{allAdd }}' wx:key='' >
            <image src='./../../image/dizhi.png'></image>
            <view class='address_right'>
                <view class='top'>
                    <text>收货人: {{item.userName}}</text>
                    <text>{{item.telNumber}}</text>
                </view>
                <view class='bottom'>收货地址: {{item.provinceName +
                 item.cityName + item.countyName + item.detailInfo}}</view>
            </view>
        </view>

      <view class='Noaddress'  bindtap='Toaddress'>
        <text class='icon'>+ 添加地址</text>
      </view>